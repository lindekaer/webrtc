{
  "name": "node",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch": "chokidar 'src/*.js' -c 'npm run build'",
    "build": "npm run transpile && npm run bundle && npm run inlineHtml && npm run inlineHtmlLocal",
    "transpile": "babel src --out-dir dist",
    "test": "npm run transpile && node dist/run.js 5",
    "inlineHtml": "inline-assets peer.html peer-inlined.html; inline-assets walker.html walker-inlined.html",
    "inlineHtmlLocal": "cp walker-inlined.html walker-inlined.local.html && replace 'SIGNALING_URL' 'ws://178.62.51.86:8080/socketserver' walker-inlined.local.html",
    "bundle": "browserify ./dist/peer.js -o ./dist/peer-bundled.js && browserify ./dist/walker.js -o ./dist/walker-bundled.js",
    "spawn": "node dist/runner.js --num-containers 3 --num-peers 10 --mode spawn",
    "walker": "node dist/runner.js --num-containers 3 --num-peers 10 --mode walker --signaling-url ws://192.168.1.144:8080/socketserver --timeout 25000",
    "full": "node dist/runner.js --num-containers 3 --num-peers 10 --mode full --signaling-url ws://192.168.1.144:8080/socketserver --timeout 25000",
    "pushit": "git add .; git commit -m 'Bacon'; git push"
  },
  "babel": {
    "presets": [
      "es2015-node",
      "stage-0"
    ]
  },
  "author": "Theodor C. Listov Lindekaer <theodor.lindekaer@gmail.com> (https://lindekaer.github.io)",
  "license": "ISC",
  "dependencies": {
    "async": "2.1.4",
    "colors": "1.1.2",
    "inline-assets": "1.1.2",
    "inliner": "1.10.1",
    "ms": "0.7.2",
    "replace": "0.3.0",
    "sdp-transform": "1.7.0",
    "selenium-webdriver": "3.0.1",
    "uuid": "3.0.1",
    "uws": "0.11.1",
    "wrtc": "0.0.61"
  },
  "devDependencies": {
    "babel": "6.5.2",
    "babel-cli": "6.18.0",
    "babel-preset-es2015-node": "6.1.1",
    "babel-preset-stage-0": "6.16.0",
    "browserify": "^13.1.1",
    "chokidar-cli": "1.2.0"
  }
}
